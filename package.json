{
  "name": "strapi-plugin-blur-placeholder",
  "version": "0.1.0",
  "description": "Blur image download for placeholder",
  "keywords": [
    "strapi",
    "plugin",
    "blurhash",
    "lqip",
    "image",
    "placeholder",
    "cloudinary",
    "sharp"
  ],
  "license": "MIT",
  "author": "MrLimmZ <contact@michelaxel.fr>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MrLimmZ/BlurPlaceholder.git"
  },
  "bugs": {
    "url": "https://github.com/MrLimmZ/BlurPlaceholder/issues"
  },
  "homepage": "https://github.com/MrLimmZ/BlurPlaceholder#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "type": "commonjs",
  "strapi": {
    "kind": "plugin",
    "name": "blur-placeholder",
    "displayName": "BlurPlaceholder",
    "description": "Blur image download for placeholder"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "admin",
    "server",
    "strapi-admin.js",
    "strapi-server.js",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.24",
    "@strapi/icons": "^2.0.0-rc.24",
    "blurhash": "^2.0.5",
    "chalk": "^4.1.2",
    "log-update": "^6.1.0",
    "node-fetch": "^3.3.2",
    "react-intl": "^7.1.11",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.14.0",
    "@strapi/typescript-utils": "^5.14.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.18",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.18"
  }
}
